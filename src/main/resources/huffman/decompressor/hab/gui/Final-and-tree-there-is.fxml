<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox
        alignment="TOP_LEFT" prefWidth="1360" prefHeight="720" spacing="20.0"
        xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="huffman.decompressor.hab.gui.TreeViewSceneController"
        stylesheets="@styles/main.css"
>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Pane prefHeight="322.0" prefWidth="338.0">
         <children>
            <Label fx:id="action_time" layoutX="14.0" layoutY="14.0" text="Action time: " />
            <Label fx:id="new_file_size" layoutX="12.0" layoutY="39.0" text="New file size: " />
            <Label fx:id="new_file_path" layoutX="11.0" layoutY="66.0" text="New file path: " />
            <Button layoutX="133.0" layoutY="214.0" mnemonicParsing="false" text="WatchTree" onAction="#switchToTreeView"/>
         </children>
      </Pane>
   </children>

    
</VBox>
